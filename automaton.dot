digraph {
  rankdir=LR;
  node [shape = doublecircle]; INIT;
  node [shape = circle];
  INIT -> INIT [ label = "..." ];
  INIT -> NUM [ label = "1..9" ];
  NUM -> NUM [ label = "0..9" ];
  NUM -> INIT [ label = "G" ];
